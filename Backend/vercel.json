{
  "version": 2,
  "builds": [
    { "src": "Frontend/html/**/*.html", "use": "@vercel/static" },
    { "src": "Frontend/css/**/*.css", "use": "@vercel/static" },
    { "src": "Frontend/image/*", "use": "@vercel/static" },
    { "src": "Frontend/js/**/*.js", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/css/(.*)", "dest": "/Frontend/css/$1" },
    { "src": "/js/(.*)", "dest": "/Frontend/js/$1" },
    { "src": "/image/(.*)", "dest": "/Frontend/image/$1" },
    { "src": "/(.*)", "dest": "/Frontend/html/$1" }
  ]
}
